#include<iostream>
using namespace std;
int main() 
{
    char vowel;
    cin>>vowel;
    if(vowel=='a'||vowel=='e'|| vowel=='i'||vowel=='o'||vowel=='u'|| vowel=='A'|| vowel=='E'|| vowel=='I'|| vowel=='O'|| vowel=='U'){
        cout<<"vowel";
    }
    else{
        cout<<"Not a vowel";
    }
return 0;
}
